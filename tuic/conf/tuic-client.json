{
    "relay": {
        "server": "my.domain.com",
        "port": 2333,
        "token": "user-token",

        "ip": "1.2.3.4",
        "certificates": ["/path/to/cert.crt"],
        "udp_relay_mode": "native",
        "congestion_controller": "cubic",
        "heartbeat_interval": 10000,
        "alpn": ["h3"],
        "disable_sni": false,
        "reduce_rtt": false,
        "request_timeout": 8000,
        "max_udp_relay_packet_size": 1500
    },
    "local": {
        "ip": "0.0.0.0",
        "port": 2334
    },
    "log_level": "INFO"
}
